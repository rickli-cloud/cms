---
import "@lucarickli/jamstack-cms/dist/style.css";
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Jamstack CMS</title>
    <meta
      name="description"
      content="Admin dashboard for managing website content"
      />
    <script>
      import config from "virtual:jamstack-cms"; 
      import { Cms } from "@lucarickli/jamstack-cms";
      new Cms({ 
        target: document.getElementById("app"),
        props: config, 
      });
      console.debug({ config })
    </script>
  </head>
  <body>
    <div id="app"></div>
  </body>
</html>
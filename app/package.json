{
  "name": "@lucarickli/jamstack-cms",
  "private": false,
  "version": "0.0.0-latest",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/app/main.d.ts",
      "browser": "./dist/app/index.umd.js",
      "require": "./dist/app/index.umd.js",
      "import": "./dist/app/index.es.js"
    },
    "./utils": {
      "types": "./dist/utils/utils.d.ts",
      "import": "./dist/utils/utils.js"
    },
    "./utils/*": "./dist/utils/*",
    "./dist/style.css": "./dist/app/style.css",
    "./dist/*": "./dist/app/*"
  },
  "scripts": {
    "dev": "vite",
    "check": "npm-run-all check:app check:utils",
    "build": "npm-run-all clean build:app build:utils",
    "preview": "vite preview",
    "clean": "rimraf ./dist",
    "check:app": "svelte-check --tsconfig ./tsconfig.json",
    "build:app": "vite build",
    "check:utils": "tsc src/utils.ts -d --outDir dist/utils --lib es2023 --module es6 --target es2022 --strict --esModuleInterop --skipLibCheck --moduleResolution node --noEmit",
    "build:utils": "tsc src/utils.ts -d --outDir dist/utils --lib es2023 --module es6 --target es2022 --strict --esModuleInterop --skipLibCheck --moduleResolution node"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.12.4",
    "autoprefixer": "^10.4.19",
    "cssnano": "^6.1.2",
    "i": "^0.3.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.7",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1"
  },
  "dependencies": {
    "@octokit/oauth-methods": "^5.1.2",
    "@octokit/request-error": "^6.1.0",
    "@octokit/rest": "^20.1.0",
    "bits-ui": "^0.21.7",
    "clsx": "^2.1.0",
    "cmdk-sv": "^0.0.17",
    "formsnap": "^1.0.0",
    "highlight.js": "^11.9.0",
    "lucide-svelte": "^0.365.0",
    "markdown-it": "^14.1.0",
    "mode-watcher": "^0.3.0",
    "monaco-editor": "^0.47.0",
    "paneforge": "^0.0.2",
    "svelte-headless-table": "^0.18.2",
    "svelte-radix": "^1.1.0",
    "svelte-spa-router": "^4.0.1",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1",
    "yaml": "^2.4.1",
    "zod": "^3.22.4"
  }
}
